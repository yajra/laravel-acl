{
  "name": "yajra/laravel-acl",
  "description": "Laravel ACL is a simple role, permission ACL for Laravel Framework.",
  "keywords": [
    "yajra",
    "laravel",
    "acl",
    "user",
    "role",
    "permissions"
  ],
  "homepage": "https://github.com/yajra/laravel-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Arjay Angeles",
      "email": "aqangeles@gmail.com",
      "homepage": "https://yajrabox.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "^9.0",
    "illuminate/database": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Yajra\\Acl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yajra\\Acl\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.0-dev"
    },
    "laravel": {
      "providers": [
        "Yajra\\Acl\\AclServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
